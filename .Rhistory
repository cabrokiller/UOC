is_euclidean <- function(diss){
mat <- as.matrix(diss)
n <- dim(mat)[1]
A <- mat * -(1/2)
H <- diag(n) - (1/n) * (rep(1,n) %*% t(rep(1,n)))
B <- H %*% A %*% H
# prueba
eig <- eigen(B)
all(eig$values >= 0)
}
